#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/clean-package@2.2.0/node_modules/clean-package/bin/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/clean-package@2.2.0/node_modules/clean-package/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/clean-package@2.2.0/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules:/Volumes/Work/chakra-ui/node_modules:/Volumes/Work/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Work/chakra-ui/chakra-ui/packages/components/menu/node_modules:/Volumes/Work/chakra-ui/chakra-ui/packages/components/node_modules:/Volumes/Work/chakra-ui/chakra-ui/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/clean-package@2.2.0/node_modules/clean-package/bin/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/clean-package@2.2.0/node_modules/clean-package/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/clean-package@2.2.0/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules/.pnpm/node_modules:/Volumes/Work/chakra-ui/chakra-ui/node_modules:/Volumes/Work/chakra-ui/node_modules:/Volumes/Work/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Work/chakra-ui/chakra-ui/packages/components/menu/node_modules:/Volumes/Work/chakra-ui/chakra-ui/packages/components/node_modules:/Volumes/Work/chakra-ui/chakra-ui/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/clean-package@2.2.0/node_modules/clean-package/bin/main.js" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/clean-package@2.2.0/node_modules/clean-package/bin/main.js" "$@"
fi
